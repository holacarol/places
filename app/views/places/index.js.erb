<% if !params[:my_page].nil? %>
	$('#place_list').html('<%= escape_javascript render(:partial => "places/places_pag", :locals => {:places_pag => @places_pag, :page => :my_page}) %>');
<% elsif !params[:friend_page].nil? %>
	$('#friends_list').html('<%= escape_javascript render(:partial => "places/places_pag", :locals => {:places_pag => @friends_pag, :page => :friend_page}) %>');
<% elsif !params[:recomm_page].nil? %>
	$('#recommended_list').html('<%= escape_javascript render(:partial => "places/places_pag", :locals => {:places_pag => @recommended_pag, :page => :recomm_page}) %>');
<% end %>

  $('.place_contacts').hide();
  $('.place_contacts_summary').tipsy({
    html: true,
    hoverable: true,
    opacity: 0.95,
    title: function(){
      return $(this).siblings('.place_contacts').html();
    }
  });

		